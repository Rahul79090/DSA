
#include<iostream>
using namespace std;
int main(){
    int arr[5];
    int size=5;
    cout<<"enter the value of array in :-"<<size<<endl;

    for(int i=0;i<size;i++){
         cin>>arr[i];
    }
   
   cout<< " you entered:-";
    for(int i=size-1;i>=0;i--){
       cout<<arr[i]<<" ";

    }
     cout<<endl;
    return 0;

}
